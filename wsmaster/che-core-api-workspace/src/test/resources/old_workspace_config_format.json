{
  "name": "default",
  "defaultEnv": "dev-env",
  "description": "This is workspace description",
  "environments": [
    {
      "name": "dev-env",
      "machineConfigs": [
        {
          "name": "dev",
          "type": "docker",
          "dev": true,
          "limits": {
            "ram": 2048
          },
          "source": {
            "location": "https://somewhere/Dockerfile",
            "type": "dockerfile"
          },
          "envVariables": {
            "env1": "value1",
            "env2": "value2"
          },
          "servers": [
            {
              "ref": "ref",
              "port": "9090/udp",
              "protocol": "protocol",
              "path": "/any/path"
            }
          ]
        },
        {
          "name": "db",
          "type": "docker",
          "dev": false,
          "limits": {
            "ram": 2048
          },
          "source": {
            "type": "image",
            "location": "codenvy/ubuntu_jdk8"
          },
          "servers": [
            {
              "ref": "ref",
              "port": "3311/tcp",
              "protocol": "protocol",
              "path": "/any/path"
            }
          ]
        },
        {
          "name": "site",
          "type": "docker",
          "dev": false,
          "limits": {
            "ram": 1024
          },
          "source": {
            "type": "dockerfile",
            "content": "FROM codenvy/ubuntu_jdk8"
          }
        }
      ]
    }
  ]
}
